{
  "name": "execution-service",
  "version": "1.0.0",
  "description": "Simple price oracle AVS in typescript",
  "author": "Tomer Nesimyan",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc",
    "start": "yarn build && node dist/main.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "start:debug": "nodemon --inspect --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "start:prod": "node dist/main.js",
    "format": "prettier --write src/"

  },
  "dependencies": {
    "axios": "^1.1.3",
    "axios-retry": "^4.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "nodemon": "^3.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "rimraf": "^6.0.1",
    "prettier": "^3.4.1",
    "typescript": "^5.6.2"
  }
}
